import random
import pandas as pd
import numpy as np

def main():

    all_data=[]
    with open('data/full_data_sequences(new)', 'r') as f:
    for sequence in f:
        all_data.append(sequence)
        
    
    n_all_user=len(all_data)
    print(n_all_user)
    
    def colab_sequence_generator(lines):

        while True:
            for j, sequence in enumerate(lines):
                sequence = sequence.split(' :: ')
                    user_id = sequence[0]
                    sequence = sequence[1:]
                    sequence = [int(sequence[3*i]) for i in range(int(len(sequence) / 3))]
     
                    yield sequence, user_id
                    
                    


if __name__ == '__main__':
    main()
